{
  "name": "tajrobeh",
  "private": true,
  "scripts": {
    "dev": "node scripts/start.js",
    "dev:host": "pnpm --filter container-app dev",
    "dev:remotes": "pnpm --filter cart-app build && pnpm --filter products-app build && pnpm --filter cart-app serve & pnpm --filter products-app serve",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
